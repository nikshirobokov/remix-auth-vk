    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "remix-auth-oauth2": "^3.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint",
